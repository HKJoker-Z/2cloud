{"ast":null,"code":"export default {\n  name: \"OpenShare\",\n  data() {\n    return {\n      code: this.$route.query.code,\n      shareId: this.$route.query.shareId,\n      share: {},\n      validateFailed: false,\n      folders: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.$request.get('/share/selectById/' + this.shareId).then(res => {\n        this.share = res.data || {};\n\n        //验证code对不对\n        if (this.share.code !== this.code) {\n          this.validateFailed = true;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","code","$route","query","shareId","share","validateFailed","folders","created","load","methods","$request","get","then","res"],"sources":["src/views/front/OpenShare.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 150px auto; text-align: center; font-size: 30px; color: #666\" v-if=\"validateFailed\">分享文件不存在或已被取消</div>\r\n    <div class=\"card\" style=\"margin: 5px auto 100px auto; width: 70%\" v-else>\r\n      <div style=\"color: #666; display: flex; align-items: center\">\r\n        分享：<strong style=\"color: #333\">{{ share.name }}</strong>\r\n        <i class=\"el-icon-time\" style=\"margin-left: 20px; margin-right: 5px\"></i> {{ share.shareTime }}\r\n        <span style=\"margin-left: 20px\">\r\n          <i class=\"el-icon-info\" style=\"margin-left: 5px\"></i>\r\n          <span v-if=\"share.status.includes('小时')\" style=\"color: red\" >{{ share.status }}</span>\r\n          <span v-else >{{ share.status }}</span>\r\n        </span>\r\n      </div>\r\n\r\n      <div>\r\n        <div style=\"padding: 15px 0; margin-top: 30px \">\r\n          <a style=\"color: #666\" :href=\"'/front/openShare?code=' + code + '&shareId=' + shareId \">全部文件 <i class=\"el-icon-arrow-right\" v-if=\"folders.length\" > </i></a>\r\n           <a style=\"color: #666\" :href=\"'/front/openShare?code=' + code + '&shareId=' + shareId + '&folderId=' + item.foldereId \" v-for=\"(item, index) in folders\" :key=\"item.id\" >\r\n             {{ item.name }}  <i class=\"el-icon-arrow-right\" v-if=\"index != folders.length - 1\"></i>\r\n           </a>\r\n        </div>\r\n\r\n        \r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"OpenShare\",\r\n  data() {\r\n    return {\r\n      code: this.$route.query.code,\r\n      shareId: this.$route.query.shareId,\r\n      share: {},\r\n      validateFailed: false,\r\n      folders: []\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.load()\r\n\r\n  },\r\n\r\n  methods: {\r\n    load() {\r\n      this.$request.get('/share/selectById/' + this.shareId).then((res) => {\r\n        this.share = res.data || {}\r\n\r\n        //验证code对不对\r\n        if (this.share.code !== this.code) {\r\n          this.validateFailed = true\r\n        }\r\n      })\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA6BA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAF,IAAA;MACAG,OAAA,OAAAF,MAAA,CAAAC,KAAA,CAAAC,OAAA;MACAC,KAAA;MACAC,cAAA;MACAC,OAAA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,IAAA;EAEA;EAEAC,OAAA;IACAD,KAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,6BAAAR,OAAA,EAAAS,IAAA,CAAAC,GAAA;QACA,KAAAT,KAAA,GAAAS,GAAA,CAAAd,IAAA;;QAEA;QACA,SAAAK,KAAA,CAAAJ,IAAA,UAAAA,IAAA;UACA,KAAAK,cAAA;QACA;MACA;IACA;EAEA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}